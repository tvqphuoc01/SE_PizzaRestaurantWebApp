        script. 
            function myFunction() {
                var x = document.getElementById("search-bar");
                var y = document.getElementById("nav-list");
                    if (x.style.display === "none") {
                        x.style.display = "block";
                        y.style.display = "none";
                    } else {
                        x.style.display = "none";
                        y.style.display = "flex";
                    }
                }
