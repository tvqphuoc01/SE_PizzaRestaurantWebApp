    body
        .container-fluid.row.mainNavBar.fixed-top.w-100.navbar-expand-lg
            //Navbar
            .col-2.d-flex.justify-content-center
                a(href="/")
                    img(src="logo.png" alt="Butter logo")
            .col-10.d-flex.justify-content-center
                ul(id="nav-list").nav.align-items-center
                    li.nav-item.dropdown <a class="home" href="/"> HOME </a> 
                        i(class="fa fa-angle-down")
                        div(class="dropdown-content")
                            a(class="dropdown-item" href="#") Bakery
                            a(class="dropdown-item" href="#") Coffee
                            a(class="dropdown-item" href="#") Pizza
                            a(class="dropdown-item" href="#") Winery
                            a(class="dropdown-item" href="#") Restaurant
                    li.nav-item SHOP
                    li.nav-item.dropdown PAGES 
                        i(class="fa fa-angle-down")
                        div(class="dropdown-content")
                            a(class="dropdown-item" href="/aboutUs") About Us
                            a(class="dropdown-item" href="#") Our Chefs
                            a(class="dropdown-item" href="/service") Service
                            a(class="dropdown-item" href="/logIn") Log in 
                            a(class="dropdown-item" href="/SignUp") Sign Up
                    li.nav-item.dropdown MENU 
                    li.nav-item RESERVATION
                    li.nav-item GALLERY
                    li.nav-item BLOG
                    li.nav-item CONTACT
                    li.nav-item
                        i(class="fas fa-cart-plus fa-lg")
                    li.nav-item
                        i(class="fas fa-search fa-lg" onclick="myFunction()")
                div.col-12(id="search-bar")
                    i(class="fas fa-search fa-lg" onclick="myFunction()")
                    input(type="text", placeholder="Search Here ..... !", )
        //- Page Header
        .page_header_login
            img.Image_Pg_Header(src="site-img12.jpg" alt="Page header")
            .pageH
                .Page_header_content
                    h2.content_header RESERVATION
                    p.content_main Book a Table...!
                //- Breadcrumb
                .Breadcrumbs
                    a.Home(href="/" style="text-decoration: none;") Home
                    i /
                    p.Login_Form(href="") Reservation
        //- Form register
        .form.Log_in(style="max-width:50%; margin:auto;")
            .frame_log_in 
                h3 RESERVATION
                .ROW 
                    .col.inside.UserName
                        input.text_login(type="username" placeholder="Username" name="username")
                        i.fas.fa-user
                    .col.inside.E-mail
                        input.text_login(type="email" placeholder="Email address" name="email")
                        i.fas.fa-envelope
                    .col.inside.Password
                        input.text_login(type="password" placeholder="Phone Number" name="password")
                        i.fas.fa-lock
                    .col.inside.Confirm
                        input.text_login(type="date" id="start" name="trip-start" value="2018-07-22" min="2020-01-01" max="2025-12-31")
                    .col.inside.Gender 
                        select.Gender_List(name="Gender") Gender
                            option(value="0" disabled=" selected disabled") Gender 
                            option(value="1") Male 
                            option(value="2") Female 
                            option(value="3") Other     
                    .col.inside.Guest 
                        select.Gender_List(name="Guest")
                            option(value="" disabled selected) Number of Guest
                            option(value="1") 1
                            option(value="2") 2
                            option(value="3") 3
                            option(value="3") 4
                            option(value="3") 5
                            option(value="3") 6
                            option(value="3") 7
                            option(value="0") More
                    .clear
                .button 
                    a.btn.register(href="#") RESERVATION NOW
