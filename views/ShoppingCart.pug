    body
        .container-fluid.row.mainNavBar.fixed-top.w-100.navbar-expand-lg
            //Navbar
            .col-2.d-flex.justify-content-center
                a(href="#")
                    img(src="logo.png" alt="Butter logo")
            .col-10.d-flex.justify-content-center
                ul(id="nav-list").nav.align-items-center
                    li.nav-item.dropdown <a class="home" href="/"> HOME </a>
                    li.nav-item 
                        a(class="contact" href="/Shop") SHOP
                    li.nav-item.dropdown PAGES 
                        i(class="fa fa-angle-down")
                        div(class="dropdown-content")
                            a(class="dropdown-item" href="/aboutUs") About Us
                            a(class="dropdown-item" href="/service") Service
                    li.nav-item.dropdown <a class="menu" href="/menu"> MENU </a>
                    li.nav-item 
                        a(class="reservation" href="/reservation") RESERVATION
                    li.nav-item 
                        a(class="Blog" href="/Blog") BLOG
                    li.nav-item
                        a(class="contact" href="/contact") CONTACT
                    li.nav-item
                        if cartLength
                            span(class="fa-stack fa-2x has-badge" data-count=cartLength)
                                a(href="/ShoppingCart")
                                    i(class="fa fa-shopping-cart fa-stack-1x fa-inverse")
                        else
                            span(class="fa-stack fa-2x has-badge" data-count=0)
                                a(href="/ShoppingCart")
                                    i(class="fa fa-shopping-cart fa-stack-1x fa-inverse")
                    li.nav-item
                        i(class="fas fa-search fa-lg" onclick="myFunction()")
                    if user
                        li.nav-item
                            a(href="/profilePage")
                                img(src=user.avatar alt="Avatar" class="avatar-nav")
                        li.nav-item.dropdown
                            a(class="contact" href="/profilePage")= user.userName
                            div(class="dropdown-content")
                                a(class="dropdown-item" href="/profilePage") Profile
                                a(class="dropdown-item" href="/logOut") Log Out
                    else
                        li.nav-item
                            a(class="contact" href="/logIn") LOGIN
                        li.nav-item
                            a(class="contact" href="/signUp") SIGN UP      
                div.col-12(id="search-bar")
                    i(class="fas fa-search fa-lg" onclick="myFunction()")
                    input(type="text" id="searchBar" placeholder="Search Here ..... !")
                    div(id="search_List")
                        ul(id="dishesList")

        //Header
        .page-header-cart
            .container
                .col-lg-6.col-md-6
                    h3.font-raleway.font-weight-700.font-size-44.white-color My Cart
                    p.font-playball.font-weight-400.font-size-34.gold-color Laughter is brightest where food is best
                .col-lg-6.col-md-6
                    .breadcrumbs
                        a(href="/" style="color:white") Home 
                        i /       
                        a Cart


        .bodyCart 
            if checkCart === true  
                .cart-style
                    if errors
                        - var n = 0;
                        each value in errors
                            .form-group 
                            .alert.alert-danger=errors[n]
                            - n++;
                    if user
                        .container.mar-top-100 
                            .row
                                .col-5.text-align-center 
                                    p.cart-title products
                                .col-2.text-align-center 
                                    p.cart-title price
                                .col-2.text-align-center 
                                    p.cart-title qty
                                .col-2.text-align-center
                                    p.cart-title total
                            ul.pad-left-0
                                - var n = 0;
                                each value in user.cart
                                    .single-line.mar-top-20.mar-bot-20
                                    li
                                        form(method="POST" action='/ShoppingCart')
                                            .row.mar-top-20.mar-bot-10
                                                input(type="text" name="Id_Mon" value=user.cart[n].Id_Mon style="display: none")
                                                .col-5.d-flex.justify-content-space-between
                                                    .col-5.flex-col.justify-content-center
                                                        img.food-img(src=user.cart[n].imgUrl)
                                                    .col-7.flex-col.justify-content-center
                                                        p.mar-bot-0.food-title=user.cart[n].dishName
                                                .col-2.flex-col.justify-content-center.text-align-center 
                                                    p.price-style.gold-color.mar-bot-0=user.cart[n].price

                                                .col-2.flex-col.justify-content-center.align-items-center
                                                    input.qty(type="text" value=user.cart[n].quantity)

                                                .col-2.flex-col.justify-content-center.text-align-center  
                                                    p.price-style.red-color.mar-bot-0=(user.cart[n].price*user.cart[n].quantity).toFixed(3)

                                                .col-1.flex-col.justify-content-center.text-align-center.cartSection.removeWrap
                                                    //- a(href='#')
                                                    //-     i.fas.fa-times.x-btn
                                                    button(class="x-btn" type="submit")
                                                        i.fa.fa-times
                                    - n++;

                .promotion-code.mar-top-70 
                    .container
                        .row.col-12
                            label.promotion-title.flex-col.justify-content-center(for ="promo") Have A Promo Code?
                        .row
                            .col-10.flex-col.justify-content-center
                                input.promotion-input(id="promo" type="text" name="promo" value= "" placeholder="Enter Code")
                            .col-2.flex-col.justify-content-center
                                a(id="promoButton" href="#" class="enter-btn btn" onclick="addPromo()" style="display: inline-block;")
                                    i.fas.fa-chevron-right

                form(method="POST" action="/placeOrder").container.mar-top-70
                    .promotion-code.mar-top-70 
                        .container
                            .row.col-12
                                label.promotion-title.flex-col.justify-content-center(for ="promo") Shipping To?
                            .row
                                .col-12.flex-col.justify-content-center
                                    input.promotion-input(id="promo" type="text" name="address" value= "" placeholder="Address")

                    .row 
                        .col-10 
                            p.small-title Subtotal 
                        .col-2 
                            if subTotals
                                p.gold-color.price-style.text-align-right.value=subTotals.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".")
                                input(type="text" name="subTotals" value=subTotals style="display: none")
                            else 
                                span.gold-color.text-align-right.value 0.0

                    .row 
                        .col-10 
                            p.small-title Shipping 
                        .col-2 
                            if subTotals
                                p.gold-color.price-style.text-align-right.value 25.000 VND
                                input(type="text" name="Shipping" value="25.000" style="display: none")
                            else 
                                p.gold-color.text-align-right.value 0.0

                    .row
                        .col-10 
                            p.small-title Tax 
                        .col-2 
                            p.price-style.gold-color.value.text-align-right 10%

                    .row#discount(style='display: none;') 
                        .col-10 
                            p.small-title Discount 
                        .col-2 
                            p.price-style.gold-color.value.text-align-right(id="Offer") 10%

                    .single-line.mar-bot-15
                    .row 
                        .col-10 
                            p.small-title Total
                        .col-2
                            p.price-style.red-color.text-align-right(id="totals").value=totals.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".")
                            input(id="totalsInput" type="text" name="Total" value=totals style="display: none") 

                    .row.mar-top-45 
                        .col-12
                            input.checkout-btn.btn(type="submit" value="Checkout")


        //-Cart
            .bodyCart 
                if checkCart === true   
                    .cart
                        h1 My Cart 
                        if errors
                            .form-group
                            .alert.alert-danger=errors
                        if user
                            ul.cartWrap
                                - var n = 0;
                                each value in user.cart
                                    if n % 2 == 0
                                        li.items.even
                                            form(method="POST" action='/ShoppingCart').row.mar-top-20.mar-bot-10
                                                input(type="text" name="Id_Mon" value=user.cart[n].Id_Mon style="display: none")
                                                .col-5.d-flex.justify-content-space-between
                                                    .col-5.flex-col.justify-content-center
                                                        img.food-img(src=user.cart[n].imgUrl)
                                                    .col-7.flex-col.justify-content-center
                                                        p.mar-bot-0.food-title=user.cart[n].dishName
                                                .col-2.flex-col.justify-content-center.text-align-center 
                                                    p.price-style.gold-color.mar-bot-0=user.cart[n].price

                                                .col-2.flex-col.justify-content-center.align-items-center
                                                    input.qty(type="text" value=user.cart[n].quantity)

                                                .col-2.flex-col.justify-content-center.text-align-center  
                                                    p.price-style.red-color.mar-bot-0=(user.cart[n].price*user.cart[n].quantity).toFixed(3)

                                                .cartSection.col-1.flex-col.justify-content-center.text-align-center.cartSection.removeWrap 
                                                    input(class="remove" type="submit" value="X")

                                        - n++;
                                    else
                                        li.items.even
                                            form(method="POST" action="/ShoppingCart").infoWrap
                                                .cartSection
                                                    img.itemImg(src=user.cart[n].imgUrl)
                                                    h3=user.cart[n].dishName
                                                    input(class="qty" type="text" value=user.cart[n].quantity)
                                                    input(type="text" name="Id_Mon" value=user.cart[n].Id_Mon style="display: none")
                                                    p.stockStatus In Stock 
                                                .cartSection(style="text-align:center") 
                                                    p(style="font-weight:bold;font-size:1.25em;")=user.cart[n].price
                                                .cartSection.removeWrap 
                                                    input(class="remove" type="submit" value="X")
                                        - n++;
                    .promoCode
                        label(for ="promo") Have A Promo Code?
                        input(id="promo" type="text" name="promo" value= "" placeholder="Enter Code")
                        a(id="promoButton" href="#" class="next-btn" onclick="addPromo()" style="display: inline-block;")
                    form(method="POST" action="/placeOrder").subtotal
                        if totals
                            ul 
                                li.total
                                    if subTotals
                                        span.label Subtotal 
                                        span.value=subTotals
                                        input(type="text" name="subTotals" value=subTotals style="display: none")
                                    else
                                        span.label Subtotal 
                                        span.value 0 VND
                                li.total
                                    if subTotals 
                                        span.label Shipping 
                                        span.value 25.000 VND
                                        input(type="text" name="Shipping" value="25.000" style="display: none")
                                li.total
                                    span.label Tax
                                    span.value 10%
                                li.total(id="discount" style="display: none;")
                                    span.label Discount
                                    span.value(id="Offer") 10%
                                li.total(style="font-size: 1.25em;font-weight: bold;")
                                    span.label Total 
                                    span(id="totals").value=totals
                                    input(id="totalsInput" type="text" name="Total" value=totals style="display: none")
                                li.total 
                                    input(class="btn" style="width:100%;text-align:center" type="submit" value="CHECK")

